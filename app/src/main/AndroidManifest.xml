<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".ComposeCardActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_card"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeSurfaceActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_surface"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeRowActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_row"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_column"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeBoxActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_box"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeImageActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_image"
            android:theme="@style/Theme.MyApplication" />
