            android:name=".system.ComposeTextFieldActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_text_field"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".system.ComposeTextActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_text"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ComposeDemoActivity"
            android:exported="true"
            android:label="Compose Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>